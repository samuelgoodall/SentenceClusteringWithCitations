@article{hyman2019matrix,
	abstract = {Abstract We develop a theoretical model for power law tailing behavior of transport in fractured rock based on the relative dominance of the decay rate of the advective travel time distribution, modeled using a Pareto distribution (with tail decaying as ∼ time−(1+α)), versus matrix diffusion, modeled using a L{\'e}vy distribution. The theory predicts that when the advective travel time distribution decays sufficiently slowly (α<1), the late-time decay rate of the breakthrough curve is −(1+α/2) rather than the classical −3/2. However, if α>1, the −3/2 decay rate is recovered. For weak matrix diffusion or short advective first breakthrough times, we identify an early-time regime where the breakthrough curve follows the Pareto distribution, before transitioning to the late-time decay rate. The theoretical predictions are validated against particle tracking simulations in the three-dimensional discrete fracture network simulator dfnWorks, where matrix diffusion is incorporated using a time domain random walk.},
	author = {Hyman, Jeffrey D. and Rajaram, Harihar and Srinivasan, Shriram and Makedonska, Nataliia and Karra, Satish and Viswanathan, Hari and Srinivasan, Gowri},
	date-added = {2021-04-14 17:08:32 -0600},
	date-modified = {2021-04-14 17:08:32 -0600},
	doi = {10.1029/2019GL085454},
	eprint = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2019GL085454},
	journal = {Geophys. Res. Lett.},
	number = {23},
	pages = {13785-13795},
	title = {Matrix Diffusion in Fractured Media: New Insights Into Power Law Scaling of Breakthrough Curves},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2019GL085454},
	volume = {46},
	year = {2019},
